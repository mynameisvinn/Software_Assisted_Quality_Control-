% insert record in limsdb

conn = database('limsdb','lims','mou53Brains!'); % establish connection

% define features
colnames = {'data_file_id', 'click_location_x_coordinate', 'click_location_y_coordinate', 'counter_id'};

% store data in array
data = {7611, 100,100, 102}; 

% combine features and data into table
data_table = cell2table(data, 'VariableNames', colnames);

% insert table to limsdb
% colnames are automatically aligned
insert(conn,'data_file_click',colnames,data_table);

% close connection
commit(conn);
close(conn);